  "author": "Fabian Dietenberger <piano.fabian@t-online.de>",
  "private": true,
  "scripts": {
    "postinstall": "webpack --progress --colors",
    "start": "node ./bin/www6",
    "start-watch": "nodemon ./bin/www6 --ignore public/ --ignore test/ --exec babel-node --presets es2015",
    "start-front": "webpack-dev-server",
    "build-front": "webpack --progress --colors",
    "test": "mocha ./test --compilers js:babel-core/register --recursive",
    "test-watch": "mocha ./test  --compilers js:babel-core/register --recursive --watch",
    "test-hypem": "mocha ./test/app/hypem  --compilers js:babel-core/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:feedm3/unhypem.git"
  },
